<script>
    const menuBtn = document.querySelector(".menu-button");
    const hamburger = document.querySelector(".menu-button__burger");
    const navigationBar = document.querySelector("#navigation-bar");
    menuBtn.addEventListener("click", toggleMenu);

    function toggleMenu() {
        hamburger.classList.toggle("open");
        navigationBar.classList.toggle("invisible");
    }

    const accordion = document.querySelectorAll(".accordion");
    const subMenu = document.querySelectorAll(".sub-menu")
    accordion.forEach(element => element.addEventListener("click", function () {
        accordion.forEach(elm => elm.children[0].classList.remove("marked")) 

        if ( element.children[1].classList.contains("visible")) {
            setTimeout(function(){  element.children[1].classList.remove("visible");
            }, 450);
            element.children[1].classList.add("non-visible");
            element.children[0].classList.remove("marked");
           
        }else{
            subMenu.forEach(countryMenu => countryMenu.classList.remove("visible"));
            element.children[1].classList.remove("non-visible");
            element.children[0].classList.add("marked");
            element.children[1].classList.add("visible")
           
        }
    }));
    let parent = document.querySelector('.navigation-bar').parentElement;

while (parent) {
const hasOverflow = getComputedStyle(parent).overflow;
if(hasOverflow !== 'visible') {
    console.log(hasOverflow, parent);
}
parent = parent.parentElement;
}
</script>
